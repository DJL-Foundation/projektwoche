{
  "extends": "prowo-tsc/react-library.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2020",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "*.config.js"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
